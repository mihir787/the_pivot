<h1>Store Admin Dashboard - Welcome <%= current_user.name %>!</h1>
  <h3><%= image_tag current_user.picture.url(:small) %></h3>
  <div class="admin-links">
    <% if current_store.users.first == current_user %>
      <p>
        <%= link_to "Add Store Admins", new_store_admin_path(store: current_store) %>
      </p>
      <p>
        <%= link_to "View All Store Admins", store_admins_path(store: current_store) %>
      </p>
    <% end %>
    <p><%= link_to "All Photos", admin_photos_path %><br></p>
    <p><%= link_to "Add New Photo", new_admin_photo_path %><br></p>
    <p><%= link_to "All Categories", admin_categories_path %><br></p>
    <p><%= link_to "New Category", new_admin_category_path %><br></p>
  </div>
