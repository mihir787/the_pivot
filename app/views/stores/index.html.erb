<div class="jumbotron">
  <h1>View Galleries and Purchase from Real Photographers</h1>
</div>
<h1>Browse the Stores</h1>
<% @stores.each do |store| %>
  <div class="col-lg-3 col-md-4 col-sm-6">
    <%= link_to "#{store.name}", store_photos_path(store: store.slug) %>
    <%= link_to image_tag(store.image), store_photos_path(store: store.slug) %>
  </div>
<% end %>
